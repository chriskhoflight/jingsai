  <div slot=\"content\">\r\n        <img :src=\"picUrl\" alt=\"暂无图片链接\" style=\"width: 100%;margin: 0 auto;display: block;\">\r\n        <a @click=\"viewImage=true\" style=\"margin-top:5px;text-align:right;display:block\">查看大图</a>\r\n      </div>\r\n    </Poptip>\r\n    <Upload\r\n      :action=\"uploadFileUrl\"\r\n      :headers=\"accessToken\"\r\n      :on-success=\"handleSuccess\"\r\n      :on-error=\"handleError\"\r\n      :format=\"['jpg','jpeg','png','gif','bmp']\"\r\n      accept=\".jpg, .jpeg, .png, .gif, .bmp\"\r\n      :max-size=\"5120\"\r\n      :on-format-error=\"handleFormatError\"\r\n      :on-exceeded-size=\"handleMaxSize\"\r\n      :before-upload=\"beforeUpload\"\r\n      :show-upload-list=\"false\"\r\n      ref=\"up\"\r\n      class=\"upload\"\r\n    >\r\n      <Button :loading=\"loading\" icon=\"ios-cloud-upload-outline\">上传图片</Button>\r\n    </Upload>\r\n\r\n    <Modal title=\"图片预览\" v-model=\"viewImage\" :styles=\"{top: '30px'}\" draggable>\r\n      <img :src=\"picUrl\" alt=\"暂无图片链接\" style=\"width: 100%;margin: 0 auto;display: block;\">\r\n      <div slot=\"footer\">\r\n        <Button @click=\"viewImage=false\">关闭</Button>\r\n      </div>\r\n    </Modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { uploadFile } from \"@/api/index\";\r\nexport default {\r\n  name: \"uploadPicInput\",\r\n  props: {\r\n    width: {\r\n      type: String,\r\n      default: \"200px\"\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      accessToken: {},\r\n      picUrl: \"\",\r\n      loading: false,\r\n      viewImage: false,\r\n      uploadFileUrl: uploadFile\r\n    };\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.accessToken = {\r\n        accessToken: this.getStore(\"accessToken\")\r\n      };\r\n    },\r\n    handleFormatError(file) {\r\n      this.loading = false;\r\n      this.$Notice.warning({\r\n        title: \"不支持的文件格式\",\r\n        desc:\r\n          \"所选文件‘ \" +\r\n          file.name +\r\n          \" ’格式不正确, 请选择 .jpg .jpeg .png .gif .bmp格式文件\"\r\n      });\r\n    },\r\n    handleMaxSize(file) {\r\n      this.loading = false;\r\n      this.$Notice.warning({\r\n        title: \"文件大小过大\",\r\n        desc: \"所选文件‘ \" + file.name + \" ’大小过大, 不得超过 5M.\"\r\n      });\r\n    },\r\n    beforeUpload() {\r\n      this.loading = true;\r\n      return true;\r\n    },\r\n    handleSuccess(res, file) {\r\n      this.loading = false;\r\n      if (res.success === true) {\r\n        file.url = res.result;\r\n        this.picUrl = res.result;\r\n        this.$emit(\"on-change\", this.picUrl);\r\n      } else {\r\n        this.$Message.error(res.message);\r\n      }\r\n    },\r\n    handleError(error, file, fileList) {\r\n      this.loading = false;\r\n      this.$Message.error(file.message);\r\n    },\r\n    handleChange(v) {\r\n      this.$emit(\"on-change\", this.picUrl);\r\n    },\r\n    setPicUrl(v) {\r\n      this.picUrl = v;\r\n      this.$emit(\"on-change\", this.picUrl);\r\n    }\r\n  },\r\n  created() {\r\n    this.init();\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.upload {\r\n  display: inline-block;\r\n  margin-left: 10px;\r\n}\r\n</style>\r\n\r\n"]}]}